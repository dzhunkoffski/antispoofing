{
    "name": "RawNet2",
    "n_gpu": 1,
    "preprocessing": {
        "sr": 16000,
        "spectrogram": {},
        "log_spec": true
    },
    "augmentations": {
        "wave": [],
        "spectrogram": []
    },
    "arch": {
        "type": "RawNet2",
        "args": {
            "n_gru_layers": 3,
            "min_low_hz": 0,
            "min_band_hz": 50,
            "abs_after_sinclayer": false
        }
    },
    "data": {
        "train": {
            "batch_size": 32,
            "num_workers": 4,
            "datasets": [
                {
                    "type": "ASVspoofDataset",
                    "args": {
                        "flac_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_train/flac",
                        "labels_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_cm_protocols/ASVspoof2019.LA.cm.train.trn.txt"
                    }
                }
            ]
        },
        "val": {
            "batch_size": 32,
            "num_workers": 4,
            "datasets": [
                {
                    "type": "ASVspoofDataset",
                    "args": {
                        "flac_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_eval/flac",
                        "labels_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_cm_protocols/ASVspoof2019.LA.cm.eval.trl.txt"
                    }
                }
            ]
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr":  0.0001,
            "weight_decay": 0.0001
        }
    },
    "loss": {
        "type": "RawNet2Loss",
        "args": {}
    },
    "metrics": [],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 150,
            "gamma": 1.0
        }
    },
    "trainer": {
        "kaggle_profile": "Profile2",
        "kaggle_version_name": "spoof ablation min_low_hz 50",
        "epochs": 150,
        "save_dir": "saved/",
        "save_period": 2,
        "verbosity": 1,
        "eval_eer_frequency": 1, 
        "monitor": "min val_loss",
        "early_stop": 100,
        "visualize": "wandb",
        "wandb_project": "antispoofing",
        "len_epoch": 2000,
        "grad_norm_clip": 100
    }
}