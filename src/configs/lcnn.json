{
    "name": "RawNet2",
    "n_gpu": 1,
    "preprocessing": {
        "sr": 16000,
        "spectrogram": {},
        "log_spec": true
    },
    "augmentations": {
        "wave": [],
        "spectrogram": []
    },
    "arch": {
        "type": "RawNet2",
        "args": {}
    },
    "data": {
        "train": {
            "batch_size": 64,
            "num_workers": 4,
            "datasets": [
                {
                    "type": "LCNNDataset",
                    "args": {
                        "flac_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_train/flac",
                        "labels_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_cm_protocols/ASVspoof2019.LA.cm.train.trn.txt",
                        "n_fft": 1724
                    }
                }
            ]
        },
        "eval": {
            "batch_size": 64,
            "num_workers": 4,
            "datasets": [
                {
                    "type": "LCNNDataset",
                    "args": {
                        "flac_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_eval/flac",
                        "labels_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_cm_protocols/ASVspoof2019.LA.cm.eval.trl.txt",
                        "n_fft": 1724
                    }
                }
            ]
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr":  3e-4,
            "weight_decay": 0
        }
    },
    "loss": {
        "type": "LCNNLoss",
        "args": {}
    },
    "metrics": [],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 10,
            "gamma": 0.5
        }
    },
    "trainer": {
        "kaggle_profile": "Profile7",
        "kaggle_version_name": "spoof ablation no wd",
        "epochs": 150,
        "save_dir": "saved/",
        "save_period": 2,
        "verbosity": 1,
        "eval_eer_frequency": 1, 
        "monitor": "min val_loss",
        "early_stop": 100,
        "visualize": "wandb",
        "wandb_project": "antispoofing",
        "len_epoch": 2000,
        "grad_norm_clip": 100
    }
}